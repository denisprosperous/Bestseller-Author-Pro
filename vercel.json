{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "framework": null,
  "routes": [
    {
      "src": "/_static/(.*)",
      "dest": "/_static/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/favicon.svg",
      "dest": "/favicon.svg"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
